<project
    name = "kgraph"
    description = "Graphenbibliothek fÃ¼r Robotik"
    script = "project.gsl"
    email = "kgraph@lists.k.org"
    >

    <version major="2" minor="1" patch="14" />

    <!--
        Includes are processed first, so XML in included files will be
        part of the XML tree
    -->
    <include filename = "license.xml" />

    <!-- Header Files -->
    <header name = "kgraph_prelude" />

    <!-- Classes that we use -->
    <class name = "kdijkstra">Dijkstra</class>

    <!-- Models that we build using GSL -->
    <model name = "sockopts" />
    <model name = "zgossip" />
    <model name = "zgossip_msg" />

    <!-- Other source files in src that we need to package -->
    <extra name = "build-android.gsl" />
    <extra name = "build-autoconf.gsl" />
    <extra name = "build-automake.gsl" />
    <extra name = "build-class.gsl" />
    <extra name = "build-cmake.gsl" />
    <extra name = "build-mingw32.gsl" />
    <extra name = "build-vs2008.gsl" />
    <extra name = "build-vs2010.gsl" />
    <extra name = "build-vs2012.gsl" />
    <extra name = "build-vs2013.gsl" />

</project>
